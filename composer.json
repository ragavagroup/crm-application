{
  "name": "medikly/application",
  "description": "Medikly main application",
  "homepage": "https://github.com/ragavagroup/application.git",
  "license": "OSL-3.0",
  "autoload": {
    "psr-0": {
      "": "src/"
    }
  },
  "repositories": {
    "composer": {
      "type": "composer",
      "url": "http://packagist.orocrm.com"
    },
    "enterprise": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/crm-enterprise.git"
    },
    "medikly-theme-2016": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/Theme2016.git"
    },
    "medikly/css-live-editor-bundle": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/CssLiveEditorBundle.git"
    },
    "medikly-entity-extend": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/MediklyEntityExtend.git"
    },
    "medikly/reports-form-bundle": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/ReportsFormBundle.git"
    },
    "medikly/system-navigation": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/SystemNavigationBundle.git"
    },
    "medikly/datagrid-charts-bundle": {
      "type": "vcs",
      "url": "git@github.com:ragavagroup/DataGridChartsBundle.git"
    },
    "medikly/channel-import-bundle" : {
      "type" : "vcs",
      "url" : "git@github.com:ragavagroup/ChannelsImportBundle.git"
    }
  },
  "require": {
    "oro/crm": "1.9.2",
    "oro/crm-enterprise": "dev-upgrade/oro-crm-1.9.2",
    "medikly/theme-2016": "dev-master",
    "medikly/css-live-editor-bundle": "dev-master",
    "medikly/system-navigation": "dev-feature/standalone-bundle",
    "medikly/reports-form-bundle": "dev-master",
    "medikly/datagrid-charts-bundle": "dev-master"
  },
  "require-dev": {
    "sensio/generator-bundle": "2.5.3"
  },
  "config": {
    "component-dir": "web/bundles/components"
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::setPermissions",
      "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::installAssets",
      "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::setAssetsVersion"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::setPermissions",
      "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::installAssets",
      "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::setAssetsVersion"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "web",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}