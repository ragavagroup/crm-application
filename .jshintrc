{
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "undef": true,
    "strict": true,
    "trailing": true,
    "latedef": "nofunc",
