{
    "name": "oro/crm-enterprise-application",
    "description": "The OroCRM Enterprise Application",
    "homepage": "https://github.com/orocrm/crm-enterprise-application.git",
    "license": "Commercial",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "repositories": {
        "package": {
            "type": "path",
            "url": "../../package/*"
        },
        "composer": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        }
    },
    "require": {
        "oro/platform": "dev-master as 1.9.x-dev",
        "oro/crm": "dev-master as 1.9.x-dev",
        "oro/crm-enterprise": "dev-master as 1.11.x-dev",
        "oro/platform-serialised-fields": "dev-master as 1.9.x-dev",
        "oro/crm-hangouts-call-bundle": "dev-master as 1.9.x-dev",
        "oro/crm-abandoned-cart": "dev-master as 1.9.x-dev",
        "oro/crm-magento-embedded-contact-us": "dev-master as 1.9.x-dev",
        "oro/crm-mail-chimp": "dev-master as 1.9.x-dev",
        "oro/crm-dotmailer": "dev-master as 1.9.x-dev",
        "oro/crm-zendesk": "dev-master as 1.9.x-dev",
        "oro/crm-pro-ldap-bundle": "dev-master as 1.9.x-dev",
        "oro/crm-pro-demo-data-bundle": "dev-master as 1.9.x-dev"
    },
    "require-dev": {
        "sensio/generator-bundle": "2.5.3"
    },
    "config": {
        "component-dir": "web/bundles/components"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::setPermissions",
            "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::installAssets",
            "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::setAssetsVersion"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::setPermissions",
            "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::installAssets",
            "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::setAssetsVersion"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
